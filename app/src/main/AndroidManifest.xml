        <activity android:name=".UnosLozinkeActivity" />
        <activity android:name=".PogresnaLozinkaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Budzet" />
        <activity android:name=".Trosak" />
        <activity android:name=".IspisTroskova" />
        <activity android:name=".IspisPodataka" />
        <activity android:name=".IspisKategorija" />
